generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model todo {
  id     Int    @id @default(autoincrement()) @db.Integer
  title  String
  detail String?
  status String?   @default("未着手")
  limit  DateTime? @db.Date
}
